{
    "singleQuote": true,
    "semi": true,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 80,
    "orderedImports": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "tailwindConfig": "./tailwind.config.js",
    "plugins": [
        "prettier-plugin-organize-imports",
        "prettier-plugin-tailwindcss"
    ]
}